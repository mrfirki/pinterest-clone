<p>hello pin new</p>

<%= form_for @pin do |f| %>
	<div class="col-md-8 col-md-offset-2">
		<%= f.label :title %>
		<%= f.text_field :title  %>
	</div>
	<div class="col-md-8 col-md-offset-2">
		<%= f.label :description %>
		<%= f.text_field :description %>
	</div>
	<div class="col-md-8 col-md-offset-2">
		<%= f.submit 'submit' %>
	</div>

<% end %>	